const callInWindow = require("callInWindow");
const setInWindow = require("setInWindow");
const injectScript = require("injectScript");
const isConsentGranted = require("isConsentGranted");
const addConsentListener = require("addConsentListener");

const userId = data.userId;
const source = data.source;

const events = [
  { name: "config", properties: { i: userId, o: source } },
  { name: "consent", properties: { consent: "revoke" } },
];

setInWindow("llo", {});
setInWindow("llo.events", events);

function injectionSuccess() {
  if (isConsentGranted("ad_user_data")) {
    callInWindow("llo.consent", "grant");
  } else {
    callInWindow("llo.consent", "revoke");
  }

  let wasCalled = false;
  addConsentListener("ad_user_data", (consentType, granted) => {
    if (wasCalled) return;
    wasCalled = true;

    if (granted) {
      callInWindow("llo.consent", "grant");
    } else {
      callInWindow("llo.consent", "revoke");
    }
  });
}

function injectionFailure() {
  return;
}

// inject the laylo pixel script
const url = "https://embed.laylo.com/pixel/laylo.js";
injectScript(url, injectionSuccess, injectionFailure, url);

// Call data.gtmOnSuccess when the tag is finished.
data.gtmOnSuccess();


